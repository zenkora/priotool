    <ImplicitUsings>disable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GenerateManifests>true</GenerateManifests>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Title>Prio Tool Background Service</Title>
